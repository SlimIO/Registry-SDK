{
    "name": "@slimio/registry-sdk",
    "version": "0.1.0",
    "description": "Node.js SDK for Registry API",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "prepublishOnly": "pkg-ok",
        "test": "node test/test.js",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "nyc npm test",
        "report": "nyc report --reporter=html"
    },
    "keywords": [
        "SlimIO",
        "Registry",
        "SDK",
        "Node.js"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "dependencies": {
        "@slimio/manifest": "^0.4.0",
        "httpie": "^1.1.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.0.0",
        "@commitlint/config-conventional": "^7.6.0",
        "@escommunity/minami": "^1.0.0",
        "@slimio/eslint-config": "^2.0.4",
        "del": "^4.1.1",
        "dotenv": "^8.0.0",
        "eslint": "^5.16.0",
        "husky": "^2.3.0",
        "isomorphic-git": "^0.55.2",
        "japa": "^2.0.10",
        "jsdoc": "^3.6.2",
        "nyc": "^14.1.1",
        "ora": "^3.4.0",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=10"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": true,
        "per-file": false,
        "lines": 98,
        "statements": 98,
        "functions": 98,
        "branches": 90
    }
}
