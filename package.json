{
    "name": "@slimio/registry-sdk",
    "version": "0.3.0",
    "description": "Node.js SDK for Registry API",
    "main": "index.js",
    "scripts": {
        "prepublishOnly": "pkg-ok",
        "test": "cross-env psp && nyc --reporter=lcov node test/test.js",
        "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
        "coverage": "codecov",
        "report": "nyc report --reporter=html"
    },
    "keywords": [
        "SlimIO",
        "Registry",
        "SDK",
        "Node.js"
    ],
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "dependencies": {
        "@slimio/manifest": "^0.9.1",
        "httpie": "^1.1.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@escommunity/minami": "^1.0.0",
        "@slimio/async-cli-spinner": "^0.5.2",
        "@slimio/eslint-config": "^4.1.0",
        "@slimio/is": "^1.5.1",
        "@slimio/psp": "^0.11.1",
        "codecov": "^3.7.0",
        "cross-env": "^7.0.2",
        "cross-spawn": "^7.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^7.0.0",
        "husky": "^4.2.5",
        "isomorphic-git": "^1.8.8",
        "japa": "^3.0.1",
        "jsdoc": "^3.6.4",
        "make-promises-safe": "^5.1.0",
        "nyc": "^15.0.1",
        "ora": "^4.0.3",
        "pkg-ok": "^2.3.1"
    },
    "engines": {
        "node": ">=12"
    },
    "husky": {
        "hooks": {
            "pre-push": "cross-env eslint index.js && npm test",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "nyc": {
        "check-coverage": false
    }
}
